{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/components/PaymentForm.jsx", "src/app/api/create-payment-intent/route.ts", "src/app/api/sendEmail/route.js", "src/app/pages/client/watch/page.jsx", "src/app/pages/administrator/video-upload/page.tsx", "src/app/api/upload-video/route.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}

